	<artifactId>tcslackbuildnotifier-rest-api</artifactId>
	<build>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.1</version>
				<configuration>
					<source>1.8</source>
					<target>1.8</target>
				</configuration>
			</plugin>
		</plugins>
	</build>
	<dependencies>
		<dependency>
			<groupId>petegoo.teamcity.plugins.tcslacknotifications</groupId>
			<artifactId>tcslackbuildnotifier-core</artifactId>
			<version>${currentVersion}</version>
			<scope>compile</scope>
		</dependency>
	</dependencies>
